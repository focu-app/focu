        "hiddenTitle": true,
        "center": true,
        "fullscreen": false,
        "resizable": false,
        "minimizable": false,
        "windowEffects": {
          "effects": ["hudWindow"],
          "state": "active"
        }
      }
    ],
    "trayIcon": {
      "id": "main_tray",
