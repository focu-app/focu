        "https://focu.app/api/latest-release"
      ]
    }
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "Focu",
        "label": "main",
        "visible": false,
        "width": 1020,
        "minWidth": 1020,
        "height": 750,
        "minHeight": 750,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "center": true,
        "fullscreen": false,
        "resizable": true,
        "decorations": true,
        "transparent": true,
        "windowEffects": {
          "effects": ["hudWindow"],
          "state": "active"
        }
      },
      {
        "title": "Tray",
