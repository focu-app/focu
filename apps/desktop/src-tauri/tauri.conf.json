  "bundle": {
    "active": true,
    "category": "Productivity",
    "copyright": "",
    "targets": "all",
    "externalBin": [
      "binaries/ollama-darwin-0.5.4"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
